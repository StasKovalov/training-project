    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
